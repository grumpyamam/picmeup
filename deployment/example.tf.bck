provider "aws" {
  access_key = "var.picmeup_aws_key_id"
  secret_key = "var.picmeup_aws_key_pwd"
  region     = "var.picmeup_aws_region"
}

resource "aws_key_pair" "deployer" {
  key_name   = "picmeup-key"
  public_key = "ssh-rsa  "
}

resource "aws_instance" "example" {
  ami           = "ami-ebd02392"
  instance_type = "t2.micro"
  key_name      = "picmeup-key"
}

